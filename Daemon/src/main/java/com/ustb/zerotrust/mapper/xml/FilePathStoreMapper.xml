<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ustb.zerotrust.mapper.FilePathStoreMapper">

    <!--插入被测软件的路径信息-->
    <select id="insertFilePath" parameterType="com.ustb.zerotrust.domain.DaemonSoft">
        INSERT INTO daemonsoft(app_name,soft_path,sign_path)
                  VALUES(#{daemonSoft.appName},#{daemonSoft.softPath},#{daemonSoft.signPath})
    </select>


</mapper>